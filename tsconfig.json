{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "backend/src",
    "frontend/src"
, "frontend/src/pages/PaginaInicio.tsx", "frontend/src/pages/Login.tsx", "frontend/src/pages/Registro.tsx", "frontend/src/pages/Staff.tsx", "frontend/src/pages/Torneos.tsx", "frontend/src/pages/Tecnificacion.tsx"  ],
  "references": [{ "path": "./tsconfig.node.json" }]
}

